{
    "dataset_args": {
        "params": {
            "limit": 1
        }
    },
    "dataloader_args": {
        "params": {
            "batch_size": 1
        }
    },
    "optimizer_args": {
        "params": {
            "lr": 1e-3
        }
    },
    "model_args": {
        "checkpoint_dir": ["path", "../models/overfit_single"]
    },
    "training_args": {
        "iterations": 100,
        "epochs_till_checkpoint": 10
    },
    "log_args": {
        "handlers": {
            "file": {
                "filename": ["path", "../log/overfit_single.txt"]
            }
        }
    }
}
